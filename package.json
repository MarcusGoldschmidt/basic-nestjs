{
  "name": "nest-basic-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run scss:build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "typeorm:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "typeorm:drop": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:drop",
    "typeorm:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "scss": "node-sass resources/scss -o public/css",
    "scss:build": "node-sass resources/scss -o public/css --output-style compressed",
    "scss:watch": "node-sass -w ./resources/scss -o ./public/css"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.11.1",
    "@nestjs/core": "^6.11.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.11.1",
    "@nestjs/typeorm": "^6.2.0",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.0.0",
    "express-react-views": "^0.11.0",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^1.0.3",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.9.1",
    "@nestjs/testing": "^6.11.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.25",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
